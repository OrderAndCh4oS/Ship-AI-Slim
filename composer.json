{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["Basic AI", "Game", "JS"],
  "homepage": "https://github.com/sarcoma/Drone-AI-Slim",
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Cooper",
            "email": "sean@orderandchaoscreative.com",
            "homepage": "https://orderandchaoscreative.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "doctrine/orm": "^2.5",
        "twig/twig": "^1.34",
        "slim/twig-view": "^2.2",
        "slim/csrf": "^0.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
      "psr-4": {
          "Oacc\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "TestUtilities\\": "test-utilities"
        }
    },
	"scripts": {
        "start": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
	}

}
